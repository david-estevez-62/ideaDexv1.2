extends layout

block content
	div.edit
		div.editblock
			p Change username?
				a(href="/"+user.username+"/changeUsername") Reset Here
			p Change password?
				a(href="/"+user.username+"/changePassword") Reset Here
			p Upload or Add Image Destination
				form(action="/uploadProfilepic", method="post", enctype="multipart/form-data")
					input(type="file", name="imageUrl")
					input(type="submit")
			p Upload or Add Image Destination
				form(action="/changeProfpic", method="post")
					input(type="text", name="imageUrl")
					input(type="submit")

		